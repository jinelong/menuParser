<script type="text/javascript">
function ShowRSSDescription(div)
{
    descriptionDivStyle = div.getElementsByTagName("div")[0].style;
    descriptionDivStyle.visibility="visible";
    descriptionDivStyle.display="block";
}
function HideRSSDescription(div)
{
    descriptionDivStyle = div.getElementsByTagName("div")[0].style;
    descriptionDivStyle.visibility="hidden";
    descriptionDivStyle.display="none";
}
